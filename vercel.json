{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
      "version": 2,
      "builds": [
        { "src": "package.json", "use": "@vercel/node" }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/api/proxy.js?path=$1"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
      ]
    }
   