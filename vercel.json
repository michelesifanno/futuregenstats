{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
      ],
        "version": 2,
        "builds": [
          {
            "src": "api/**/*.js",
            "use": "@vercel/node"
          }
        ],
        "routes": [
          {
            "src": "/api/(.*)",
            "dest": "/api/$1"
          }
        ],
          "engines": {
            "node": "20.x"
          }
      }
   